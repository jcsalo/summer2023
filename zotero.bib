}

@article{kapiteijn638,
  title = {Preoperative Radiotherapy Combined with Total Mesorectal Excision for Resectable Rectal Cancer},
  author = {Kapiteijn, E. and Marijnen, C. A. and Nagtegaal, I. D. and Putter, H. and Steup, W. H. and Wiggers, T. and Rutten, H. J. and Pahlman, L. and Glimelius, B. and {van Krieken}, J. H. and Leer, J. W. and {van de Velde}, C. J. and {Dutch Colorectal Cancer Group}},
