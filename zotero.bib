@article{bae32,
  title = {Intravenous Contrast Medium Administration and Scan Timing at {{CT}}: Considerations and Approaches},
  shorttitle = {Intravenous Contrast Medium Administration and Scan Timing at {{CT}}},
  author = {Bae, Kyongtae T.},
  year = {2010},
