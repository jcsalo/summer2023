  shorttitle = {Oxaliplatin-{{Based Chemotherapy}} in {{Patients}} with {{Metastatic Colorectal Cancer Aged}} at {{Least}} 75 {{Years}}},
  author = {Rosati, Gerardo and Cordio, Stefano and Reggiardo, Giorgio and Aprile, Giuseppe and Butera, Alfredo and Avallone, Antonio and Tucci, Aniello and Novello, Giuseppe and Blanco, Giuseppina and Caputo, Giuseppe and Bilancia, Domenico and Bordonaro, Roberto},
  year = {2019},
